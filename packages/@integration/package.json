{
  "name": "@marblejs/integration",
  "version": "2.3.1",
  "description": "Marble.js integration tests",
  "main": "src/index.ts",
  "scripts": {
    "start:http": "ts-node --project tsconfig.json src/http/index.ts",
    "start:websockets": "ts-node --project tsconfig.json src/websockets/http.server.ts",
    "start:messaging:server": "ts-node --project tsconfig.json src/messaging/server.ts",
    "start:messaging:client": "ts-node --project tsconfig.json src/messaging/client.ts",
    "watch:dev": "nodemon --watch src --ext ts --ignore '*.spec.ts' --exec yarn start",
    "test": "jest --config ../../jest.config.js",
    "clean": "rimraf dist"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marblejs/marble.git"
  },
  "author": "JÃ³zef Flakus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marblejs/marble/issues"
  },
  "homepage": "https://github.com/marblejs/marble#readme",
  "dependencies": {
    "@marblejs/core": "^2.3.1",
    "@marblejs/middleware-body": "^2.3.1",
    "@marblejs/middleware-cors": "^2.3.1",
    "@marblejs/middleware-io": "^2.3.1",
    "@marblejs/middleware-logger": "^2.3.1",
    "@marblejs/middleware-multipart": "^2.3.1",
    "@marblejs/websockets": "^2.3.1",
    "@types/node": "^10.1.2",
    "rxjs": "^6.3.3",
    "typescript": "~3.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "ts-node": "^7.0.1",
    "concurrently": "^4.1.0"
  }
